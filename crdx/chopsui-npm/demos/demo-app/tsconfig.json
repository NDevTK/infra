{
  "compilerOptions": {
    "baseUrl": "./",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "lib": ["es2017", "dom", "dom.iterable"],
    "module": "ES6",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist",
    "rootDir": "./",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "tsBuildInfoFile": ".tsbuildinfo",
  },
  "exclude": [],
  "include": [
    "**/*.ts"
  ]
}
