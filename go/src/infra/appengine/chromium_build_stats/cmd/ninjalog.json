{
    "type": "record",
    "name": "ninjalog",
    "fields": [
        {
            "name": "build_id",
            "type": "long"
        },
        {
            "name": "targets",
            "type": {
                "type": "array",
                "items": "string"
            }
        },
        {
            "name": "step_name",
            "type": "string",
            "default": ""
        },
        {
            "name": "jobs",
            "type": "int"
        },
        {
            "name": "os",
            "type": "string"
        },
        {
            "name": "cpu_core",
            "type": "int"
        },
        {
            "name": "build_configs",
            "type": {
                "type": "array",
                "items": {
                    "name": "build_config",
                    "type": "record",
                    "fields": [
                        {
                            "name": "key",
                            "type": "string"
                        },
                        {
                            "name": "value",
                            "type": "string"
                        }
                    ]
                }
            }
        },
        {
            "name": "log_entries",
            "type": {
                "type": "array",
                "items": {
                    "name": "log_entry",
                    "type": "record",
                    "fields": [
                        {
                            "name": "outputs",
                            "type": {
                                "type": "array",
                                "items": "string"
                            }
                        },
                        {
                            "name": "start_duration_sec",
                            "type": "double"
                        },
                        {
                            "name": "end_duration_sec",
                            "type": "double"
                        },
                        {
                            "name": "weighted_duration_sec",
                            "type": "double"
                        }
                    ]
                }
            }
        },
        {
            "name": "created_at",
            "type": {
                "type": "long",
                "logicalType": "timestamp-micros"
            }
        }
    ]
}
