[
  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[CACHE]/builder/build_wheels"
    ],
    "infra_step": true,
    "name": "init cache if it doesn't exist"
  },
  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "verify"
    ],
    "cwd": "[CACHE]/builder/build_wheels",
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "gclient verify"
  },
  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "config",
      "--spec",
      "cache_dir = '[CACHE]/git'\nsolutions = [{'deps_file': '.DEPS.git', 'managed': True, 'name': 'infra', 'url': 'https://chromium.googlesource.com/infra/infra.git'}]"
    ],
    "cwd": "[CACHE]/builder/build_wheels",
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "gclient setup",
    "timeout": 600
  },
  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "sync",
      "--verbose",
      "--nohooks",
      "-j8",
      "--reset",
      "--force",
      "--upstream",
      "--no-nag-max",
      "--with_branch_heads",
      "--with_tags",
      "--delete_unversioned_trees",
      "--revision",
      "infra@origin/main",
      "--output-json",
      "/path/to/tmp/json"
    ],
    "cwd": "[CACHE]/builder/build_wheels",
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "gclient sync",
    "timeout": 600,
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"solutions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"infra/\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": \"5e03a58dc0850e5137a8ebbce29610670c756aed\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@\"5e03a58dc0850e5137a8ebbce29610670c756aed\"@@@"
    ]
  },
  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "recurse",
      "git",
      "config",
      "user.name",
      "local_bot"
    ],
    "cwd": "[CACHE]/builder/build_wheels",
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "gclient recurse (git config user.name)",
    "timeout": 600
  },
  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "recurse",
      "git",
      "config",
      "user.email",
      "local_bot@example.com"
    ],
    "cwd": "[CACHE]/builder/build_wheels",
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "gclient recurse (git config user.email)",
    "timeout": 600
  },
  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[CACHE]/builder/build_wheels",
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "[CACHE]/builder/build_wheels/[CACHE]/builder/build_wheels/infra/go/src/go.chromium.org/luci/build/GO_VERSION",
      "/path/to/tmp/"
    ],
    "cwd": "[CACHE]/builder/build_wheels/infra",
    "env": {
      "DISTUTILS_USE_SDK": "1",
      "MSSdk": "1"
    },
    "infra_step": true,
    "name": "read [CACHE]/builder/build_wheels/infra/go/src/go.chromium.org/luci/build/GO_VERSION",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@GO_VERSION@6.6.6@@@",
      "@@@STEP_LOG_END@GO_VERSION@@@"
    ]
  },
  {
    "cmd": [
      "cipd",
      "ensure",
      "-root",
      "[CACHE]/go6_6_6",
      "-ensure-file",
      "infra/3pp/tools/go/${platform} version:2@6.6.6",
      "-max-threads",
      "0",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "cwd": "[CACHE]/builder/build_wheels/infra",
    "env": {
      "DISTUTILS_USE_SDK": "1",
      "MSSdk": "1"
    },
    "infra_step": true,
    "name": "ensure_installed",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"\": [@@@",
      "@@@STEP_LOG_LINE@json.output@      {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"instance_id\": \"resolved-instance_id-of-version:2@6.6.6-\", @@@",
      "@@@STEP_LOG_LINE@json.output@        \"package\": \"infra/3pp/tools/go/resolved-platform\"@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    ]@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "go",
      "build",
      "-o",
      "[CLEANUP]/.goexe_tmp_1/buildwheel",
      "[CACHE]/builder/build_wheels/infra/go/src/infra/experimental/buildwheel"
    ],
    "cwd": "[CACHE]/builder/build_wheels/infra/go/src/infra",
    "env": {
      "DISTUTILS_USE_SDK": "1",
      "GO111MODULE": "on",
      "GOBIN": "[CACHE]/gocache/bin",
      "GOCACHE": "[CACHE]/gocache/cache",
      "GOMODCACHE": "[CACHE]/gocache/modcache",
      "GOPACKAGESDRIVER": "off",
      "GOPATH": null,
      "GOPRIVATE": "*.googlesource.com,*.google.com",
      "GOPROXY": null,
      "GOROOT": "[CACHE]/go6_6_6",
      "MSSdk": "1"
    },
    "env_prefixes": {
      "PATH": [
        "[CACHE]/go6_6_6/bin"
      ]
    },
    "env_suffixes": {
      "PATH": [
        "[CACHE]/gocache/bin"
      ]
    },
    "name": "build go build_wheel binary"
  },
  {
    "cmd": [
      "[CLEANUP]/.goexe_tmp_1/buildwheel",
      "--output",
      "[CLEANUP]/build.json",
      "--",
      "--root",
      "[CLEANUP]/.dockerbuild_tmp_1",
      "wheel-build"
    ],
    "cwd": "[CACHE]/builder/build_wheels/infra",
    "env": {
      "DISTUTILS_USE_SDK": "1",
      "GO111MODULE": "on",
      "GOBIN": "[CACHE]/gocache/bin",
      "GOCACHE": "[CACHE]/gocache/cache",
      "GOMODCACHE": "[CACHE]/gocache/modcache",
      "GOPACKAGESDRIVER": "off",
      "GOPATH": null,
      "GOPRIVATE": "*.googlesource.com,*.google.com",
      "GOPROXY": null,
      "GOROOT": "[CACHE]/go6_6_6",
      "MAC_CHROMIUM_TMPDIR": "[CLEANUP]/tmp_tmp_1",
      "MSSdk": "1",
      "TEMP": "[CLEANUP]/tmp_tmp_1",
      "TEMPDIR": "[CLEANUP]/tmp_tmp_1",
      "TMP": "[CLEANUP]/tmp_tmp_1",
      "TMPDIR": "[CLEANUP]/tmp_tmp_1"
    },
    "env_prefixes": {
      "PATH": [
        "[CACHE]/go6_6_6/bin"
      ]
    },
    "env_suffixes": {
      "PATH": [
        "[CACHE]/gocache/bin"
      ]
    },
    "name": "launch luciexe binary for dockerbuild",
    "stdin": "\u0012\u00002\u0006\b\ufffd\u00af\ufffd\u0005:\u0006\b\ufffd\u00af\ufffd\u0005`\u0002",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@step.sub_build@{@@@",
      "@@@STEP_LOG_LINE@step.sub_build@  \"id\": \"1234\",@@@",
      "@@@STEP_LOG_LINE@step.sub_build@  \"status\": \"SUCCESS\",@@@",
      "@@@STEP_LOG_LINE@step.sub_build@  \"steps\": [@@@",
      "@@@STEP_LOG_LINE@step.sub_build@    {@@@",
      "@@@STEP_LOG_LINE@step.sub_build@      \"logs\": [@@@",
      "@@@STEP_LOG_LINE@step.sub_build@        {@@@",
      "@@@STEP_LOG_LINE@step.sub_build@          \"name\": \"log\",@@@",
      "@@@STEP_LOG_LINE@step.sub_build@          \"url\": \"step/0/log/0\"@@@",
      "@@@STEP_LOG_LINE@step.sub_build@        },@@@",
      "@@@STEP_LOG_LINE@step.sub_build@        {@@@",
      "@@@STEP_LOG_LINE@step.sub_build@          \"name\": \"stdout\",@@@",
      "@@@STEP_LOG_LINE@step.sub_build@          \"url\": \"step/0/log/1\"@@@",
      "@@@STEP_LOG_LINE@step.sub_build@        },@@@",
      "@@@STEP_LOG_LINE@step.sub_build@        {@@@",
      "@@@STEP_LOG_LINE@step.sub_build@          \"name\": \"stderr\",@@@",
      "@@@STEP_LOG_LINE@step.sub_build@          \"url\": \"step/0/log/2\"@@@",
      "@@@STEP_LOG_LINE@step.sub_build@        }@@@",
      "@@@STEP_LOG_LINE@step.sub_build@      ],@@@",
      "@@@STEP_LOG_LINE@step.sub_build@      \"name\": \"dockerbuild\",@@@",
      "@@@STEP_LOG_LINE@step.sub_build@      \"status\": \"SUCCESS\"@@@",
      "@@@STEP_LOG_LINE@step.sub_build@    }@@@",
      "@@@STEP_LOG_LINE@step.sub_build@  ]@@@",
      "@@@STEP_LOG_LINE@step.sub_build@}@@@",
      "@@@STEP_LOG_END@step.sub_build@@@"
    ]
  },
  {
    "name": "$result"
  }
]