[
  {
    "cmd": [
      "cipd",
      "ensure",
      "-root",
      "[CACHE]",
      "-ensure-file",
      "experimental/jairogarciga_at_google.com/purple_panda latest",
      "-max-threads",
      "0",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "name": "ensure_installed",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"\": [@@@",
      "@@@STEP_LOG_LINE@json.output@      {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"instance_id\": \"resolved-instance_id-of-latest----------\", @@@",
      "@@@STEP_LOG_LINE@json.output@        \"package\": \"experimental/jairogarciga_at_google.com/purple_panda\"@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    ]@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "ls",
      "[CACHE]"
    ],
    "name": "Check what we have"
  },
  {
    "cmd": [
      "python3",
      "-u",
      "RECIPE_MODULE[depot_tools::git]/resources/git_setup.py",
      "--path",
      "[CACHE]",
      "--url",
      "https://chrome-internal.googlesource.com/infra/infra_internal"
    ],
    "name": "git setup"
  },
  {
    "cmd": [
      "git",
      "fetch",
      "origin",
      "main",
      "--progress"
    ],
    "cwd": "[CACHE]",
    "env": {
      "PATH": "RECIPE_REPO[depot_tools]:<PATH>"
    },
    "infra_step": true,
    "name": "git fetch"
  },
  {
    "cmd": [
      "git",
      "checkout",
      "-f",
      "FETCH_HEAD",
      "--",
      "infra/infra_internal/+/refs/heads/main/infra_internal/tools/purple_panda/Python/mac_vm_script.py"
    ],
    "cwd": "[CACHE]",
    "infra_step": true,
    "name": "git checkout"
  },
  {
    "cmd": [
      "git",
      "rev-parse",
      "HEAD"
    ],
    "cwd": "[CACHE]",
    "infra_step": true,
    "name": "read revision",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>checked out 'deadbeef'<br/>@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "clean",
      "-f",
      "-d",
      "-x"
    ],
    "cwd": "[CACHE]",
    "infra_step": true,
    "name": "git clean"
  },
  {
    "cmd": [
      "git",
      "submodule",
      "sync"
    ],
    "cwd": "[CACHE]",
    "infra_step": true,
    "name": "submodule sync"
  },
  {
    "cmd": [
      "git",
      "submodule",
      "update",
      "--init",
      "--recursive"
    ],
    "cwd": "[CACHE]",
    "infra_step": true,
    "name": "submodule update"
  },
  {
    "cmd": [
      "ls",
      "[CACHE]"
    ],
    "name": "Check Again"
  },
  {
    "cmd": [
      "echo",
      "[CACHE]/Purple_Panda-Swift"
    ],
    "name": "Print Hello World"
  },
  {
    "cmd": [
      "python3",
      "~/cr/infra_internal/infra_internal/tools/purple_panda/Python/mac_vm_script.py",
      " --binary_path",
      "binary_loc",
      "--cpu_count",
      "4",
      "--bundle",
      "/dummy_loc"
    ],
    "name": "Single VM"
  },
  {
    "name": "$result"
  }
]