      "@@@STEP_LOG_LINE@json.output@    \"got_revision_superproject\": \"3a52ce780950d4d969792a2559cd519d7ee8c727\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision_superproject_cp\": \"refs/heads/main@{#205336}\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
