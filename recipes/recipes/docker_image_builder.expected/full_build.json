      "@@@STEP_LOG_LINE@json.output@        \"git_checkout\": {@@@",
      "@@@STEP_LOG_LINE@json.output@          \"repo_url\": \"https://fake.org/..git\", @@@",
      "@@@STEP_LOG_LINE@json.output@          \"revision\": \"3a52ce780950d4d969792a2559cd519d7ee8c727\"@@@",
      "@@@STEP_LOG_LINE@json.output@        }@@@",
      "@@@STEP_LOG_LINE@json.output@      }, @@@",
