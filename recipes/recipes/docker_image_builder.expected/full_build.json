  {
    "cmd": [
      "vpython3",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[CACHE]/builder",
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "which",
      "docker"
    ],
    "name": "ensure docker installed"
  },
  {
    "cmd": [
      "docker",
      "version"
    ],
    "name": "docker version",
    "~followup_annotations": [
      "@@@STEP_TEXT@1.2.3@@@"
    ]
  },
  {
    "cmd": [],
    "name": "clear old images"
  },
  {
    "cmd": [
      "docker",
      "images",
      "-q",
      "-f",
      "reference=android_docker:*"
    ],
    "name": "clear old images.list images",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "docker",
      "rmi",
      "-f",
      "img1"
    ],
    "name": "clear old images.delete image img1",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "/bin/bash",
      "[CACHE]/builder/infra/docker/android_devices_ARM/build.sh"
    ],
    "name": "build image"
  },
  {
    "cmd": [
      "luci-auth",
      "token",
      "-scopes",
      "https://www.googleapis.com/auth/cloud-platform",
      "-lifetime",
      "3m"
    ],
    "infra_step": true,
    "name": "get access token for default account"
  },
  {
    "cmd": [
      "python3",
      "-u",
      "RECIPE_MODULE[infra::docker]/resources/docker_login.py",
      "--server",
      "gcr.io",
      "--service-account-token-file",
      "extra.secret.token.should.not.be.logged",
      "--config-file",
      "[CLEANUP]/.docker"
    ],
    "name": "docker login"
  },
  {
    "cmd": [
      "docker",
      "--config",
      "[CLEANUP]/.docker",
      "tag",
      "android_docker:latest",
      "gcr.io/chops-public-images-prod/android_docker:2012-05-14-12-53"
    ],
    "name": "docker tag"
  },
  {
    "cmd": [
      "docker",
      "--config",
      "[CLEANUP]/.docker",
      "push",
      "gcr.io/chops-public-images-prod/android_docker:2012-05-14-12-53"
    ],
    "name": "docker push",
    "~followup_annotations": [
      "@@@STEP_TEXT@image = android_docker:1-2-3: digest: sha256:deadbeef size:123@@@"
    ]
  },
  {
    "name": "$result"
  }
]