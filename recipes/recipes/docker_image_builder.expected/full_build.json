      "@@@SET_BUILD_PROPERTY@got_revision_cp@\"refs/heads/main@{#197293}\"@@@",
      "@@@SET_BUILD_PROPERTY@got_revision_superproject@\"3a52ce780950d4d969792a2559cd519d7ee8c727\"@@@",
