[
  {
    "cmd": [
      "cipd",
      "ensure",
      "-root",
      "RECIPE_REPO[depot_tools]/.cipd_bin",
      "-ensure-file",
      "RECIPE_REPO[depot_tools]/cipd_manifest.txt",
      "-max-threads",
      "0",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "cwd": "None",
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "name": "ensure depot_tools/.cipd_bin",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": {}@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "number",
      "abcdefabcdef63ad814cd1dfffe2fcfc9f81299c"
    ],
    "cwd": "None",
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "get commit label"
  },
  {
    "cmd": [
      "echo",
      "11112-abcdefa"
    ],
    "name": "label 1"
  },
  {
    "cmd": [
      "git",
      "number",
      "abcdefabcdef63ad814cd1dfffe2fcfc9f81299c"
    ],
    "cwd": "None",
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "get commit label (2)"
  },
  {
    "cmd": [
      "echo",
      "abcdefa-unknown-11112-2012.05.14-0"
    ],
    "name": "label 2"
  },
  {
    "name": "$result"
  }
]