[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]\\resources\\fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "rmtree",
      "[START_DIR]\\git"
    ],
    "infra_step": true,
    "name": "rmtree workdir"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]\\resources\\pycurl.py",
      "--url",
      "https://api.github.com/repos/git-for-windows/git/releases/latest",
      "--status-json",
      "/path/to/tmp/json",
      "--outfile",
      "/path/to/tmp/json"
    ],
    "name": "get latest release"
  },
  {
    "cmd": [
      "cipd.bat",
      "search",
      "infra/git/windows-amd64",
      "-tag",
      "version:2.12.2.2.chromium14",
      "-json-output",
      "/path/to/tmp/json",
      "-service-account-json",
      "C:\\creds\\service_accounts\\service-account-cipd-builder.json"
    ],
    "name": "cipd search infra/git/windows-amd64 version:2.12.2.2.chromium14",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": [@@@",
      "@@@STEP_LOG_LINE@json.output@    {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"instance_id\": \"resolved-instance_id-of-instance_id_1---\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"package\": \"infra/git/windows-amd64\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "import sys; sys.exit(0)"
    ],
    "name": "Synced",
    "~followup_annotations": [
      "@@@STEP_TEXT@Package is up to date.@@@"
    ]
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]